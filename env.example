# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Firebase AI Logic no requiere API key adicional
# Se autentica autom√°ticamente con Firebase

# Firebase AI Logic Region (optional, defaults to us-central1)
VITE_FIREBASE_AI_REGION=us-central1

# Google Places API Configuration
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Google Search API Configuration
VITE_GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
VITE_GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# MCP Configuration
# Google Cloud Service Account (opcional, para acceso completo a Firebase)
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Google Cloud Project ID (opcional)
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id

# MCP Ports (opcional)
BROWSER_MCP_PORT=3001
FIREBASE_MCP_PORT=3002
GOOGLE_CLOUD_MCP_PORT=3003
